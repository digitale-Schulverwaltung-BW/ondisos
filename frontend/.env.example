# Frontend .env.example

# Backend API URL (where anmeldungen are submitted)
BACKEND_API_URL=http://localhost/backend/api

# Email settings
FROM_EMAIL=noreply@example.com
MAIL_HEAD=Eine neue Anmeldung ist eingegangen.
MAIL_FOOT=Mit freundlichen Grüßen

# CORS (comma-separated list of allowed origins)
ALLOWED_ORIGINS=http://localhost,https://example.com

# File upload limits
UPLOAD_MAX_SIZE=10485760
UPLOAD_ALLOWED_TYPES=pdf,jpg,jpeg,png,gif,doc,docx,xls,xlsx